{
    "version": 2,
    "env": {
        "DATABASE_URL": "@DATABASE_URL",
        "BASE_URL": "@BASE_URL",
        "PORT": "@PORT",
        "REDIS_HOST": "@REDIS_HOST",
        "REDIS_PORT": "@REDIS_PORT",
        "JWT_SECRET": "@JWT_SECRET",
        "CLIENT_PORT": "@CLIENT_PORT"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}